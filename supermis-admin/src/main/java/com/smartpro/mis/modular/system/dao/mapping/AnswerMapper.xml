<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smartpro.mis.modular.system.dao.AnswerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.smartpro.mis.modular.system.model.Answer">
        <id column="id" property="id" />
        <result column="megOwner" property="megOwner" />
        <result column="answerId" property="answerId" />
        <result column="question" property="question" />
        <result column="answer" property="answer" />
        <result column="isReply" property="isReply" />
        <result column="qaTime" property="qaTime" />
        <result column="replyTime" property="replyTime" />
        <result column="isValid" property="isValid" />
        <result column="alterCon" property="alterCon" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, megOwner, answerId, question, answer, isReply, qaTime, replyTime, isValid, alterCon
    </sql>

</mapper>
